@article{richerson_homage_1997,
	title = {Homage to {Malthus}, {Ricardo}, and {Boserup}: {Toward} a {General} {Theory} of {Population}, {Economic} {Growth}, {Environmental} {Deterioration}, {Wealth}, and {Poverty}},
