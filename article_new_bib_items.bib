	keywords = {Animals, Male, Mice, Mice, Inbred BALB C, Mice, Inbred C57BL, Skin Transplantation, Transplantation, Homologous, Immune Tolerance, T-Lymphocytes, Mice, Knockout, Graft Rejection, Cell Differentiation, Adoptive Transfer, Inflammation, Heart Transplantation, Mice, Inbred C3H, Mice, Nude, Wound Healing},
	pages = {3065--3071}
}

@article{loconto_participatory_2018,
	title = {Participatory {Guarantee} {Systems}: {Alternative} {Ways} of {Defining}, {Measuring}, and {Assessing} ‘{Sustainability}’},
	volume = {58},
	copyright = {© 2017 The Authors. Sociologia Ruralis © 2017 European Society for Rural Sociology.},
	issn = {1467-9523},
	shorttitle = {Participatory {Guarantee} {Systems}},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1111/soru.12187},
	doi = {10.1111/soru.12187},
	abstract = {Over the past 20 years, standards and certification have become the leading governance mechanism for determining what sustainability entails, how to measure it, and how to assess it. This system of sustainability standards has generally relied upon the third-party certification (TPC) model to ensure that producers are complying with standards. Over the past ten to fifteen years, critiques of this model have emerged in both practitioner and academic circles that question the appropriateness of this model based on the type of knowledge that is privileged, the marginalisation of some actors, and the allocation of accountability to individual rather than collective actors. We draw upon case studies from Japan and Chile to examine the ways that participatory guarantee systems (PGS) institute practices for defining, measuring, and assessing sustainability that empower local actors – both producers and consumers. Our cases illustrate that expert and lay knowledges are both relevant and often have different strengths. We argue that PGS offer an alternative approach to sustainability governance, one that may be more democratic and hence, produce forms of sustainability that incorporate the lived experiences of people around the world.},
	language = {en},
	number = {2},
	urldate = {2019-02-10},
	journal = {Sociologia Ruralis},
	author = {Loconto, Allison and Hatanaka, Maki},
	year = {2018},
	pages = {412--432},
	file = {Full Text PDF:/Users/mayalapp/Zotero/storage/ZJBK8TZU/Loconto and Hatanaka - 2018 - Participatory Guarantee Systems Alternative Ways .pdf:application/pdf;Snapshot:/Users/mayalapp/Zotero/storage/8N2PSBID/soru.html:text/html}
}

