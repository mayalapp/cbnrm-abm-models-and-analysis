	file = {ScienceDirect Full Text PDF:/Users/mayalapp/Zotero/storage/LLMVM4D3/Bilandzija et al. - 2018 - Evaluation of Croatian agricultural solid biomass .pdf:application/pdf;ScienceDirect Snapshot:/Users/mayalapp/Zotero/storage/RB4WXSGD/S1364032118303848.html:text/html}
}

